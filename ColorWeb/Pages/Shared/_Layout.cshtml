<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no" />
    <title>@ViewData["Title"] - ColorWeb</title>

    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
    <link href="~/css/site.min.css?rel=14fb5db1f0" rel="stylesheet" />
    <link href="~/font/fontello/css/fontello-embedded.css" rel="stylesheet" />


    <environment include="Development">
    </environment>
    <environment exclude="Development">
    </environment>
</head>
<body class="preload">

    <div id="idnavbutton" class="hamburger hamburger--arrow" onclick="toggleNav();">
        <span class="hamburger-box">
            <span class="hamburger-inner"></span>
        </span>
    </div>

    <div id="idsitetitle" class="mh-sitetitle">
        <h1>&nbsp;Colors&nbsp;&nbsp;</h1>
    </div>

    <div id="idnavsidebar" class="sidenav">
        <br />
        <a class="navlink" href="/" onclick="gotoNav('/'); return false;">Home</a>
        <a class="navlink" href="/" onclick="gotoNav('/config'); return false;">Config</a>
        <br /><br />
        <div class="navtext">Provided "as is" with no warranties, and confer no rights.</div>
        <br />
        <div class="navtext"><a href="https://github.com/markharrisonuk" target="_blank"><i class="icon-github-circled navicons"></i></a>&nbsp;<a href="https://markharrison.io" class="navtextlink" target="_blank">Mark&nbsp;Harrison</a></div>
        <div class="navtext"><a href="https://azure1.io" target="_blank"><i class="icon-azure navicons"></i></a>&nbsp;<a href="https://azure1.io" class="navtextlink" target="_blank">Azure&nbsp;1</a></div>

    </div>

    <div id="idnavoverlay" onclick="toggleNav();"></div>

    <div id="idmaincontent">
        <div class="container-fluid ">
            <br /><br /><br /><br />
            @RenderBody()
            <hr style="border-top: 2px solid black" />
            <footer>
                <p style="float:right">Mark&nbsp;Harrison</p>
            </footer>
        </div>
    </div>

    <script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
    <script src="~/js/site.min.js?rel=82f48df47a"></script>

    <script>
        $(document).ready(function () {
            $("body").removeClass("preload");
        });
    </script>

    @RenderSection("Scripts", required: false)

</body>
</html>
